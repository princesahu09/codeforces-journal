import java.util.*; public class Program228A{       public static void main(String args[])      {            Scanner sc=new Scanner(System.in);            int n1=sc.nextInt();            int n2=sc.nextInt();            int n3=sc.nextInt();            int n4=sc.nextInt();            Set<Integer> set=new HashSet<>();            set.add(n1);            set.add(n2);            set.add(n3);                        set.add(n4);            System.out.println(4-set.size());       }}