#include<iostream>using namespace std;int main(){       int testcase;       cin>>testcase;       while(testcase--)      {             int a,b,c,d;            cin>>a>>b>>c>>d;            if(a==b&&b==c&&c==d&&d==a)            {                  cout<<"YES"<<endl;                                          }            else            {                  cout<<"NO"<<endl;            }      }}