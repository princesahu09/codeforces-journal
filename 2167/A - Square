import java.util.*; public class Main{       public static void main(String args[])      {            Scanner sc=new Scanner(System.in);            int testcase=sc.nextInt();             while(testcase>0)            {                  testcase--;                  int a=sc.nextInt();                  int b=sc.nextInt();                  int c=sc.nextInt();                  int d=sc.nextInt();                   if(a==b&&b==c&&c==d&&d==a)                  {                        System.out.println("YES");                  }                  else                  {                        System.out.println("NO");                   }            }      }}