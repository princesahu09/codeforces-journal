#include<iostream>using namespace std; int main(){       int t;      cin>>t;      while(t-- >0)      {              int n ,k;            cin>>n>>k;             bool status=false;             for(int i=0;i<n;i++)            {                  int temp;                  cin>>temp;                  if(temp==k)                  {                        status=true;                  }            }             if(status)            {                  cout<<"YES"<<endl;            }            else            {                  cout<<"NO"<<endl;            }      }       return 0;}